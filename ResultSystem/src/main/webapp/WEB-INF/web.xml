	</servlet>
	<servlet-mapping>
		<servlet-name>LoadStudent</servlet-name>
		<url-pattern>/LoadStudent</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddMarks</servlet-name>
